#include <stdio.h

int add(int a, int b)
{
    return a + b;
}

int sub(int a, int b)
{
    return a - b;
}

int mul(int a, int b)
{
    return a * b;
}

int divide(int a, int b)
{
    if (b == 0)
    {
        return 0; // Avoid divide-by-zero crash
    }
    return a / b;
}

int main()
{
    int a = 12;
    int b = 4;

    printf("Add: %d\n", add(a, b));
    printf("Sub: %d\n", sub(a, b));
    printf("Mul: %d\n", mul(a, b));
    printf("Div: %d\n", divide(a, b));

    return 0;
}

